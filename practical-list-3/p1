
import java.util.*;

class Parents {

	int num = 5;
}

class Child extends Parents {
	
	int num = 10;

	void display()
	{
		System.out.println("parents class num value :" + num);
		System.out.println("Child class num value :" + super.num);
	}
}

	public class M1
	{
	
	public static void main(String[] args)
	{
System.out.println("Pinak viradiya");
		System.out.println("220130318014");
		Child obj = new Child();
		obj.display();
	}
 
}
	
	

